                          <path d="M12 0c-6.627 0-12 5.373-12 12s5.373 12 12 12 12-5.373 12-12-5.373-12-12-12zm6.278 7.372l-2.434 12.51c-.237 1.219-1.176 1.56-2.219.816l-3.86-3.016-1.792 2.948c-.236.388-.608.562-1.008.468l.276-4.042 6.623-5.827c.34-.298-.098-.458-.522-.106l-7.571 6.309-3.276-.843c-1.173-.302-1.137-1.208.137-1.562l14.27-4.034c1.017-.287 1.838.254 1.465 1.272z" />
                        </svg> Telegram
                      </span>
                    </div>
                  </div>
                  
                  <Button 
                    type="submit"
                    disabled={isSubmitting}
                    className="w-full bg-jify-primary-500 hover:bg-jify-primary-600 py-4 font-semibold"
                  >
                    {isSubmitting ? 'Submitting...' : 'Submit Enquiry'}
                    <ArrowRightIcon className="w-5 h-5 ml-2" />
                  </Button>
                </form>
              </div>
            </div>
          </div>
        </div>
      </section>
    </div>
  );
}